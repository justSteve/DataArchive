{
  "name": "data-archive",
  "version": "1.0.0",
  "description": "Drive cataloging and archival system with TypeScript API and Python domain logic",
  "main": "dist/index.js",
  "scripts": {
    "build": "bun run tsc",
    "build:frontend": "bun run vite build",
    "watch": "bun run tsc --watch",
    "dev": "bun run vite",
    "api": "bun run dist/api/index.js",
    "start": "bun run api",
    "dev:all": "concurrently \"bun run api\" \"bun run dev\"",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "drive-cataloging",
    "archival",
    "file-system",
    "wsl",
    "bun"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.10.6",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.3.3",
    "vite": "^7.1.10"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@myorg/api-server": "file:../packages/api-server",
    "@myorg/dashboard-ui": "file:../packages/dashboard-ui",
    "axios": "^1.6.5",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ws": "^8.16.0"
  }
}
